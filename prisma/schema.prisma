generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Building {
  id                 String   @id @default(cuid())
  name               String
  x                  Int
  y                  Int
  z                  Int
  schematicFileName  String
  materialsFileName  String
  screenshotDataUrl  String?
  materials          Json
  createdAt          DateTime @default(now())
}
